<div id="<%= dom_id card %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">User:</strong>
    <%= card.user_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">English firstname:</strong>
    <%= card.english_firstname %>
  </div>
  <div>
    <strong class="block font-medium mb-1">English lastname:</strong>
    <%= card.english_lastname %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Chinese name:</strong>
    <%= card.chinese_name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Title:</strong>
    <%= card.title %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Chinese:</strong>
    <%= card.chinese %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Company name:</strong>
    <%= card.company_name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Company address:</strong>
    <%= card.company_address %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Chinese company address:</strong>
    <%= card.chinese_company_address %>
  </div>
</div>
